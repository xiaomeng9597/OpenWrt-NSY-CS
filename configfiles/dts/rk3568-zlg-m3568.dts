&spi0 {
	pinctrl-names = "default";
	max-freq = <48000000>;
	pinctrl-0 = <&spi0m1_cs0 &spi0m1_pins>;
	status = "okay";
	spidev@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "rockchip,spidev";
		spi-max-frequency = <120000000>;
		reg = <0>;
	};   
};

&spi2 {
	status = "okay";
	max-freq = <48000000>;
	pinctrl-names = "default";
	pinctrl-0 = <&spi2m1_pins &spi2m1_cs0 &spi2m1_cs1>;
	ads7846@0 {
		compatible = "ti,tsc2046";
		reg = <0 1>;
		interrupt-parent = <&gpio3>;
		interrupts = <RK_PB4 IRQ_TYPE_LEVEL_LOW>;
		pendown-gpio = <&gpio3 RK_PB4 GPIO_ACTIVE_LOW>;
		spi-cpol;
		spi-max-frequency = <1500000>;
		ti,keep-vref-on;
	};

	ads7846@0 {
		compatible = "ti,tsc2046";
		reg = <0 1>;
		interrupt-parent = <&gpio3>;
		interrupts = <RK_PB0 IRQ_TYPE_LEVEL_LOW>;
		pendown-gpio = <&gpio3 RK_PB0 GPIO_ACTIVE_LOW>;
		spi-cpol;
		spi-max-frequency = <1500000>;
		ti,keep-vref-on;
	};
};

&hdmi {
	avdd-0v9-supply = <&vdda0v9_image>;
	avdd-1v8-supply = <&vcca1v8_image>;
	status = "okay";
};

&hdmi_in {
	hdmi_in_vp0: endpoint {
		remote-endpoint = <&vp0_out_hdmi>;
	};
};

&hdmi_out {
	hdmi_out_con: endpoint {
		remote-endpoint = <&hdmi_con_in>;
	};
};

&hdmi_sound {
	status = "okay";
};

&pinctrl {
	headphone {
		hp_det: hp-det {
			rockchip,pins = <1 RK_PB2 RK_FUNC_GPIO &pcfg_pull_down>;
		};
	};

	leds {
		user_led_pin: user-status-led-pin {
			rockchip,pins = <0 RK_PD6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	pmic {
		pmic_int: pmic_int {
			rockchip,pins =
				<0 RK_PA3 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	pcie {
		vcc3v3_pcie_en: vcc3v3-pcie-en {
			rockchip,pins = <0 RK_PD4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	usb {
		vcc5v0_host_en: vcc5v0-host-en {
			rockchip,pins = <3 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		vcc5v0_otg_en: vcc5v0-otg-en {
			rockchip,pins = <4 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
};

